<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HelloWorld.Entry">
    <ContentPage.Content>
        <StackLayout>
            <Entry Keyboard="Text" Placeholder="First Name"></Entry>
            <Entry Keyboard="Text" Placeholder="Second Name"></Entry>
            <Entry Keyboard="Numeric"  Placeholder="Phone Number"></Entry>
            <Entry IsPassword="True" Placeholder="Password"></Entry>
            <Button Text="Next" TextColor="Blue" Clicked="Button_OnClicked"></Button>
            <Button Text="Back" TextColor="Black" Clicked="Button_OnClicked1"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>