<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HelloWorld.ApplicationProperties">
    <StackLayout>
        <TableView>
      <TableRoot>
          <TableSection>
                <EntryCell x:Name="title" Label="Name Title"  
                           Placeholder="Write your name here" Completed="Onchange"></EntryCell >
                <EntryCell x:Name="two" Label="Password" Placeholder="Type your password here" Completed="Onchange"></EntryCell>
                <SwitchCell x:Name="notificationsEnabled" Text="Notifications" OnChanged="Onchange"
                            On="False"></SwitchCell>
            </TableSection>
      </TableRoot>
  </TableView>
    <Button Text="Next" TextColor="Blue" Clicked="Button_OnClicked"></Button>
    <Button Text="Back" TextColor="Black" Clicked="Button_OnClicked1"></Button>
    </StackLayout>
</ContentPage>